{
  "ConnectionStrings": {
    "Storage": "UseDevelopmentStorage=true"
  },
  "Logging": {
    "LogLevel": {

      "Default": "Warning",
      "System": "Information",
      "Microsoft": "Information",

      "Function.ProcessMaskedEmailCommandAsync": "Warning",
      "Function.ProcessMaskedEmailCommandAsync.User": "Trace",

      "Host.Triggers.Queue": "None",

      "Microsoft.Azure.WebJobs.Host.Queues.Listener.QueueListener": "Warning",
      "Microsoft.Azure.WebJobs.Hosting.JobHostService": "Warning",
      "Microsoft.Extensions.Hosting.Internal.Host": "Warning",
      "Microsoft.Extensions.Hosting.JobHostService": "Warning"

    },
    "Console": {
      "IncludeScopes": false,
    },
    "Syslog": { "LogLevel": { "Default": "Trace" } }
  },
  "SyslogSettings": {
    "MessageTransportProtocol": "Udp",
    "HeaderType": "Rfc5424v1",
    "FacilityType":  "Local3"
  }
}